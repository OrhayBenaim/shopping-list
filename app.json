{
  "expo": {
    "name": "shopping-list",
    "slug": "shopping-list",
    "scheme": "shopping-list",
    "version": "2.1.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0076CE"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ottersproduction.shoppinglist"
    },
    "android": {
      "versionCode": 24,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0076CE"
      },
      "package": "com.ottersproduction.shoppinglist"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-asset",
        {
          "assets": ["./assets/images/intro.png", "./assets/images/share.png"]
        }
      ],
      "expo-font",
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Shopping list to access your camera",
          "recordAudioAndroid": false
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you add an image to a shopping list item."
        }
      ],
      "expo-localization",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6a0ae512-323d-488e-86bc-75dc58436ed8"
      }
    }
  }
}
